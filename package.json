{
  "name": "invictusgo",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "engines": {
    "node": "12.18.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run front\"",
    "server": "nodemon --exec babel-node server/index.js",
    "clean": "rm -rf node_modules",
    "clean:front": "npm run clean --prefix front",
    "clean:all": "concurrently \"npm run clean\" \"npm run clean:front\"",
    "install:front": "npm install --prefix front",
    "front": "npm run start --prefix front",
    "build:front": "npm run build --prefix front",
    "build:server": "babel server --out-dir build",
    "build": "concurrently \"npm run build:server\" \"npm run build:front\"",
    "postinstall": "npm run install:front",
    "heroku-postbuild": "npm run build",
    "format:server": "prettier --write server/**/**/**/*.js",
    "format:front": "prettier --write front/src/**/**/*.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.2",
    "concurrently": "^6.2.0",
    "connect-mongo": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.11",
    "mongoose-hidden": "^1.9.1",
    "mongoose-paginate": "^5.0.3",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0"
  }
}
